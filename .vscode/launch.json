            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeArgs": [],
            "env": {
                "NODE_ENV": "development"
            },
            "linux": {
                "runtimeExecutable": "${env.HOME}/.nodebrew/node/v6.9.4/bin/electron"
            },
            "windows": {
                "runtimeExecutable": "${env.APPDATA}/npm/node_modules/electron-prebuilt/dist/electron.exe"
            },
            "console": "integratedTerminal",
            "sourceMaps": false,
            "outFiles": []
      }
